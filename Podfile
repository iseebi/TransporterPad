  pod 'EBIAXMLDecompressor', :git => 'https://github.com/iseebi/EBIAXMLDecompressor.git'
  pod 'Alamofire', '~> 4.8'
  pod 'Swinject', '~> 2.6'
  pod 'SwinjectStoryboard', '~> 2.2'
end
